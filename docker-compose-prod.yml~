version: '3'

services:

  battleofheroes:
    container_name: battle_of_heroes
    build: .
    labels:
      - traefik.enable=true
      - traefik.backend=battleofheroes
      - traefik.frontend.rule=Host:laloupe.wilders.dev;PathPrefixStrip:/stargate
      - traefik.docker.network=web
      - traefik.port=80
    networks:
      - web

networks:
  web:
    external: true